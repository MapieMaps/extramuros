<div class="container-fluid">
  <div class="row">

    <div class="col-12 col-lg-6">

      <div class="row coworkings-list">

        <div class="row coworking-fields">
          <div class="col-6 col-lg-6">
            <form action="#" class="row">
              <div class="col-md-12">
                <div class="form-group">
                  <input type="text" class="form-control" id="input">
                </div>
              </div>
            </form>
          </div>
          <div class="col-6 col-lg-6">
            <form action="#" class="row">
              <div class="col-md-12">
                <div class="form-group">
                  <input type="text" class="form-control" id="input">
                </div>
              </div>
            </form>
          </div>
        </div>

        <% @coworkings.each do |coworking| %>
          <div class="coworking-card">
            <%= cl_image_tag(coworking.photos.first.key)%>
            <div>
              <h2><%= coworking.name %></h2>
              <p><%= coworking.address %></p>
              <small><%= coworking.equipements %></small>
            </div>
          </div>
        <% end %>
      </div>
    </div>

    <div class="col-12 col-lg-6" style="background-color: lightseagreen;">
      map
    </div>

  </div>
</div>





<% @coworkings.each do |coworking| %>
  <% if coworking.avatar.attached? %>
    <%= image_tag coworking.avatar %>
  <% end %>
  <p><small><%= coworking.name %></small></p>
  <%= coworking.contact_first_name %>
  <%= cl_image_tag(coworking.photos.first.key)%>

<% end %>


